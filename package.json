{
  "name": "projnodii",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./dist-server/bin/www",
    "wa-dev": "cross-env NODE_ENV=development babel-watch --watch ./server/views ./server/bin/www",
    "we-ser": "cross-env NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "cleans": "rimraf dist-server",
    "cleanc": "rimraf public/js",
    "bu-ser": "npm run cleans && babel ./server --out-dir dist-server --copy-files",
    "bu-cli": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "npm run bu-cli && npm run bu-ser"
  },
  "dependencies": {
    "cookie-parser": "~1.4.5",
    "debug": "~4.3.2",
    "express": "~4.17.1",
    "hbs": "^4.1.1",
    "http-errors": "~1.8.0",
    "morgan": "~1.10.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.1",
    "babel-watch": "^7.6.0",
    "core-js": "^3.19.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.0",
    "eslint": "^8.1.0",
    "eslint-webpack-plugin": "^3.1.0",
    "mini-css-extract-plugin": "^2.4.3",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-hot-middleware": "^2.25.1"
  }
}
